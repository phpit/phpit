{
  "name": "phpit/phpit",
  "description": "a php library",
  "keywords": ["php", "library", "iterator"],
  "homepage": "https://github.com/phpit/phpit",
  "license": "MIT",
  "authors": [
    {"name": "Demjan Kaluzki", "email": "kaluzkidemjan@gmail.com"}
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "it\\": ["src/it"]
    },
    "files": [
      "src/it/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "it\\": ["tests/it"]
    }
  }
}
